(defn dec (x) (- x 1))

;factorial, defined with define and lambda
(define factorial
    (lambda (n)
        (if (= n 0)
            1
            (* n (factorial (dec n))))))

; fibonacci, defined with defn
(defn fibonacci (n)
    (if (<= n 2)
        1
        (+ (fibonacci (- n 1))
           (fibonacci (- n 2)))))

